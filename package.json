{
  "name": "syncro-space-monorepo",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    ".",
    "space/client",
    "space/server"
  ],
  "scripts": {
    "dev": "concurrently --names=\"MAIN,CAVE_CLIENT\" --prefix-colors=\"cyan,magenta\" \"npm run dev:main\" \"npm run dev:cave-client\"",
    "dev:main": "next dev -p 9002",
    "dev:cave-client": "cd space/client && npm run dev -- --port 3001",
    "dev:cave-server": "cd space/server && npm run dev",
    "dev:all": "concurrently --names=\"MAIN,CAVE_CLIENT,CAVE_SERVER\" --prefix-colors=\"cyan,magenta,yellow\" \"npm run dev:main\" \"npm run dev:cave-client\" \"npm run dev:cave-server\"",
    "build": "npm run build:main && npm run build:cave-client && npm run build:cave-server",
    "build:main": "NODE_OPTIONS='--max_old_space_size=8192' NEXT_TELEMETRY_DISABLED=1 next build",
    "build:cave-client": "cd space/client && npm run build",
    "build:cave-server": "cd space/server && npm run build",
    "start": "concurrently --names=\"MAIN,CAVE_CLIENT,CAVE_SERVER\" --prefix-colors=\"cyan,magenta,yellow\" \"npm run start:main\" \"npm run start:cave-client\" \"npm run start:cave-server\"",
    "start:main": "next start -p 9002",
    "start:cave-client": "cd space/client && npx serve -s dist -l 3001",
    "start:cave-server": "cd space/server && npm start",
    "install:all": "npm install && cd space/client && npm install && cd ../server && npm install && cd ../..",
    "status": "node scripts/check-services.js",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "build:fast": "NODE_OPTIONS='--max_old_space_size=8192 --max-semi-space-size=512' NEXT_TELEMETRY_DISABLED=1 next build",
    "build:static": "NODE_OPTIONS='--max_old_space_size=8192' NEXT_TELEMETRY_DISABLED=1 next build",
    "build:analyze": "ANALYZE=true NODE_OPTIONS='--max_old_space_size=8192' next build",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "bigquery:setup": "tsx scripts/setup-bigquery.ts",
    "bigquery:sync": "tsx scripts/sync-data.ts",
    "bigquery:test": "tsx scripts/test-bigquery.ts",
    "clean": "rm -rf .next node_modules/.cache",
    "prebuild": "npm run clean"
  },
  "dependencies": {
    "@excalidraw/excalidraw": "^0.17.6",
    "@genkit-ai/googleai": "^1.15.0",
    "@genkit-ai/next": "^1.15.0",
    "@google-cloud/bigquery": "^8.1.1",
    "@google-cloud/bigquery-data-transfer": "^5.1.0",
    "@hookform/resolvers": "^4.1.3",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-menubar": "^1.1.1",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@reduxjs/toolkit": "^2.9.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@tailwindcss/vite": "^4.1.13",
    "@types/peerjs": "^1.1.0",
    "@types/uuid": "^10.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "colyseus.js": "^0.16.19",
    "date-fns": "^3.6.0",
    "dotenv": "^16.5.0",
    "embla-carousel-react": "^8.6.0",
    "firebase": "^11.10.0",
    "framer-motion": "^11.2.10",
    "fs": "^0.0.1-security",
    "genkit": "^1.15.0",
    "immer": "^10.1.3",
    "lucide-react": "^0.544.0",
    "next": "^15.5.4",
    "next-themes": "^0.4.6",
    "peerjs": "^1.5.5",
    "phaser": "^3.90.0",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-firebase-hooks": "^5.1.1",
    "react-hook-form": "^7.54.2",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-redux": "^9.2.0",
    "recharts": "^3.2.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.16.2",
    "use-debounce": "^10.0.1",
    "uuid": "^13.0.0",
    "web-push": "^3.6.7",
    "zod": "^3.24.2",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.18",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "eslint": "9.36.0",
    "eslint-config-next": "15.5.4",
    "genkit-cli": "^1.15.0",
    "handlebars-loader": "^1.7.3",
    "ignore-loader": "^0.1.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.1.5",
    "typescript": "^5"
  }
}
