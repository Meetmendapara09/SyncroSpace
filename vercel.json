{
  "buildCommand": "npm run build:vercel",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "build": {
    "env": {
      "NODE_OPTIONS": "--max_old_space_size=8192"
    }
  },
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1"
  }
}